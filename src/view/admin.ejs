<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/admin.css">
    <title>Adminstracion</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    

    
</head>
<body>
    <nav class="navbar navbar-dark bg-dark">
        <a href="#" class="navbar-brand mx-auto">Hotel Super Fun</a>
    </nav>
    <div class="">
        <a href="/"></a>
        <div class="">
            <table class="table ">
                <tr>
                    <th>Rut</th>
                    <th>Telefono</th>
                    <th>e-mail</th>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Accion</th>
                </tr>
                <%for(var i = 0; i < data_cliente.length ;i++){%>
                <tr>
                    <td><%= data_cliente[i].rut_cliente %></td>
                    <td><%= data_cliente[i].telefono %></td>
                    <td><%= data_cliente[i].email %></td>
                    <td><%= data_cliente[i].nombre %></td>
                    <td><%= data_cliente[i].apellido %></td>
                    <td>
                        <form action="/delete_cliente/<%= data_cliente[i].rut_cliente %>">
                            <input type="submit" value="Eliminar">
                        </form>
                    </td>

                </tr>
                <% } %>
            </table>
            <form action="/agregar_cliente">
                <input type="submit" value="Agregar un Cliente">
            </form>
<!---          <form action="/test">
                <input type="submit" value="test">
            </form>
--> 
        </div>
        
    </div>

</body>
</html>